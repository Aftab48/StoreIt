{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "standard",
    "prettier",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quotes": "off",
    "@typescript-eslint/no-require-imports": "off",
    "no-undef": "off",
    "tailwindcss/no-custom-classname": "off"
  }
}
